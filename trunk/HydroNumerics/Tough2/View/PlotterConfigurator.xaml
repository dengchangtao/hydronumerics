<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" x:Class="HydroNumerics.Tough2.View.PlotterConfigurator"
    Title="PlotterConfigurator" Height="300" Width="300" mc:Ignorable="d">
	<Grid>
		<Label Content="Horizontal axis label:"/>
		<TextBox x:Name="tx" Margin="120.5,0,8,0" VerticalAlignment="Top" Height="24" d:LayoutOverrides="VerticalMargin" />
		<Label Content="Vertical axis label:" Margin="0,40,0,-40"/>
		<TextBox x:Name="ty" Margin="120.5,40,8,0" VerticalAlignment="Top" Height="24" />
	</Grid>
</Window>

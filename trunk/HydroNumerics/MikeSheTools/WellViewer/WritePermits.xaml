<Window x:Class="HydroNumerics.MikeSheTools.WellViewer.WritePermits"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Export Mike She files for pumping data using the permits" Height="300" Width="300">
    <Grid>
        <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label>Which year should be used to distribute extraction on active intakes?</Label>
            <TextBox></TextBox>
        </StackPanel>

            <StackPanel Orientation="Horizontal">
                <Label>Start year:</Label>
                <TextBox x:Name="StartYear"></TextBox>

                <Label>End year:</Label>
                <TextBox x:Name="EndYear"></TextBox>

            </StackPanel>


        </StackPanel>
    </Grid>
</Window>

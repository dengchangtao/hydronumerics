<UserControl x:Class="DateTimeAxisTutorial.Page"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:d3="clr-namespace:Microsoft.Research.DynamicDataDisplay;assembly=DynamicDataDisplay.SilverLight"
    Width="900" Height="800">
    <Grid x:Name="LayoutRoot" Background="White">
        <StackPanel Orientation="Vertical">
        <Button Height="20" Width="80" Click="Button_Click">Tryk</Button>
        <d3:ChartPlotter Name="plotter" Width="600" Height="600" VerticalAlignment="Top">

            <d3:Header FontFamily="Georgia" Content="DateTimeAxis Tutorial"/>
            <d3:VerticalAxisTitle FontFamily="Georgia">
                <TextBlock>Vertical axis</TextBlock></d3:VerticalAxisTitle>
            <d3:HorizontalAxisTitle FontFamily="Georgia">                <TextBlock>DateTime axis</TextBlock></d3:HorizontalAxisTitle>
        </d3:ChartPlotter>
        </StackPanel>
    </Grid>
</UserControl>
